{
  "name": "react-vite-app-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p dev:vite hcm",
    "dev:vite": "vite",
    "build": "run-s build:hcm build:tsc build:vite",
    "build:hcm": "hcm src/**/*.module.css",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "preview": "vite preview",
    "storybook": "run-p dev-storybook hcm",
    "dev-storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "hcm": "hcm src/**/*.module.css --watch",
    "new:fc": "hygen component new",
    "lint": "run-s lint:ts lint:css lint:format",
    "fix": "run-s fix:ts fix:css fix:format",
    "lint:ts": "eslint .",
    "fix:ts": "eslint . --fix",
    "lint:css": "stylelint src/**/*.css",
    "fix:css": "stylelint src/**/*.css --fix",
    "lint:format": "prettier . --check",
    "fix:format": "prettier . --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@storybook/addon-docs": "^7.0.6",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/addon-styling": "^1.0.1",
    "@storybook/blocks": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-vite": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react": "4.0.0",
    "@vitest/coverage-c8": "^0.30.1",
    "babel-loader": "9.1.2",
    "clsx": "1.2.1",
    "destyle.css": "^4.0.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-define-config": "1.18.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-mdx": "2.0.5",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.11",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-unused-imports": "2.0.0",
    "happy-css-modules": "2.1.0",
    "happy-dom": "^9.9.2",
    "hygen": "6.2.11",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.23",
    "postcss-preset-env": "8.3.2",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.6",
    "stylelint": "15.5.0",
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "typescript": "5.0.4",
    "vite": "4.3.0",
    "vitest": "^0.30.1"
  },
  "volta": {
    "node": "18.16.0"
  }
}
